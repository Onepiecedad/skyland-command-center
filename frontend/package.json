    "@tanstack/react-query-devtools": "^5.17.0",
    "axios": "^1.6.2",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.302.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.21.1",
    "remark-gfm": "^4.0.1",
    "three": "^0.182.0",
    "zod": "^3.22.4"
